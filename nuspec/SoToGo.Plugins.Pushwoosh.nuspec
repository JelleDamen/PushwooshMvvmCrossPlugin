<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>SoToGo.Plugins.Pushwoosh</id>
		<version>1.0.0-beta1</version>
		<title>Pushwoosh plugin for MvvmCross</title>
		<authors>Jelle Damen</authors>
		<owners>JelleDamen</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/JelleDamen/PushwooshMvvmCrossPlugin</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Pushwoosh plugin for MvvmCross
MvvmCross plugin that wraps the native Pushwoosh SDK</description>
		<tags>mvvm mvvmcross cross xamarin android ios pushwoosh pushnotification notification push monodroid monotouch xamarin.android xamarin.ios xamarin.forms net net45 netcore plugin</tags>
		<iconUrl>https://cp.pushwoosh.com/img/logo.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Xamarin.Android.Support.v4" version="23.0.1.3" />
				<dependency id="Xamarin.GooglePlayServices.Gcm" version="27.0.0.0" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
			</group>
			<group targetFramework ="Xamarin.iOS">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
			</group>
			<group targetFramework ="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
			</group>
		</dependencies>
	</metadata>
	<files>		
		<!-- PCL -->
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Droid/bin/Release/SoToGo.Plugins.Pushwoosh.*" target="lib/portable-win+net45+wp8+win8+wpa81" />
		
		<!-- dotnet -->
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Droid/bin/Release/SoToGo.Plugins.Pushwoosh.*" target="lib/dotnet" />
		
		<!-- droid -->
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Droid/bin/Release/SoToGo.Plugins.Pushwoosh.*" target="lib/MonoAndroid" />
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Droid/bin/Release/SoToGo.Plugins.Pushwoosh.Droid.*" target="lib/MonoAndroid" />
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Droid/bin/Release/SoToGo.Plugins.Pushwoosh.Droid.PushwooshSDK.*" target="lib/MonoAndroid" />
		<file src="droid_content/PushwooshPluginBootstrap.cs.pp" target="content/MonoAndroid/Bootstrap/PushwooshPluginBootstrap.cs.pp" />

		<!-- touch -->
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Touch/bin/Release/SoToGo.Plugins.Pushwoosh.*" target="lib/Xamarin.iOS10" />
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Touch/bin/Release/SoToGo.Plugins.Pushwoosh.Touch.*" target="lib/Xamarin.iOS10" />
		<file src="../SoToGo.PushwooshMvvmCross/SoToGo.Plugins.Pushwoosh.Touch/bin/Release/SoToGo.Plugins.Pushwoosh.Touch.PushwooshSDK.*" target="lib/Xamarin.iOS10" />
		<file src="touch_content/PushwooshPluginBootstrap.cs.pp" target="content/Xamarin.iOS10/Bootstrap/PushwooshPluginBootstrap.cs.pp" />

	</files>
</package>









